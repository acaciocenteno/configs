# git aliases
alias a="sudo apt"
alias g="git"
alias gs="git status -sb"
alias ga="git add"
alias gc="git commit"
alias gps="git push"
alias gpl="git pull"
alias gl="git log --all --graph --decorate --oneline --abbrev-commit"
alias gr="git rebase"
alias grc="git rebase --continue"
alias grs="git rebase --skip"
alias org="emacs ~/.cur.org"
alias h="sudo halt"
